    "# 1816. 截断句子\n",
    "# https://leetcode-cn.com/problems/truncate-sentence/\n",
    "\n",
    "def truncateSentence(s: str, k: int) -> str:\n",
    "    return \" \".join(s.split()[:k])\n",
    "\n",
    "s = \"Hello how are you Contestant\"\n",
    "k = 4\n",
    "truncateSentence(s,k)"
