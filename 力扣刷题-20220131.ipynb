   "source": [
    "# 1342. 将数字变成 0 的操作次数\n",
    "# https://leetcode-cn.com/problems/number-of-steps-to-reduce-a-number-to-zero/\n",
    "\n",
    "def numberOfSteps(num: int) -> int:\n",
    "    ans = 0\n",
    "    while num:\n",
    "        ans += num & 1\n",
    "        if num > 1:\n",
    "            ans += 1\n",
    "        num >>= 1\n",
    "    return ans\n",
    "numberOfSteps(14)"
   ]
  }
