   "source": [
    "# 476. 数字的补数\n",
    "# https://leetcode-cn.com/problems/number-complement/\n",
    "\n",
    "# 这个题目简单\n",
    "def findComplement(num: int) -> int:\n",
    "    result = 0\n",
    "    t = 1\n",
    "    while num > 0:\n",
    "        result += (1 if num&1 == 0 else 0)*t\n",
    "        num >>= 1\n",
    "        t *= 2    \n",
    "    return result\n",
    "    \n",
    "findComplement(1)\n"
   ]
  }
