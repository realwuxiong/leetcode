    "# 575. 分糖果\n",
    "# https://leetcode-cn.com/problems/distribute-candies/\n",
    "\n",
    "from typing import List\n",
    "def distributeCandies(candyType: List[int]) -> int:\n",
    "    m = len(set(candyType))\n",
    "    n = len(candyType)\n",
    "    return n // 2 if m >= n //2 else m\n",
    "candies = [1,1,2,3]\n",
    "distributeCandies(candies)"
   ]
  }
